#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0)
#define ll long long int
#define ld long double
#define vec vector<ll>
#define pb push_back
#define mp make_pair
#define sz size()
#define f(i,a,b) for(i=a;i<b;i++)
using namespace std;
const int N = 4e5+10;
int curr[N]={0};
signed main()
{
    ll q,i,j;
    cin>>q;
    while(q--)
    {
        ll n,sum=0,x=0;
        cin>>n;
        while(n--)
        {
            ll k;
            cin>>k;
            sum+=k;
            x=x^k;
        }
        if(sum==2*x)cout<<0<<"\n";
        else cout<<2<<"\n"<<x<<" "<<sum+x;
        cout<<"\n";
    }
    return 0;
}
  
